{
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "consistent-return": 0,
    "class-methods-use-this": 0,
    "import/extensions": 0,
    "import/first": 0,
    "import/no-extraneous-dependencies": 0,
    "max-len": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0
  }
}
